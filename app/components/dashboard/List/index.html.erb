<div class="w-full flex mt-8 flex-wrap <%= helpers.border_color %>">
  <div class="w-full border-b">
    <%= render Headers::Text.new(title: "#{@title}", small: true, with_margin: false, with_border: false, font: "small", padding_class: "p-4") %>
  </div>
  <div class="w-full flex flex-wrap divide-y divide-gray-300">
    <% @items.each do |item| %>
      <div class="w-full flex p-4 gap-8">
        <div class="w-1/5">
          <p
            title="<%= item[:title] %>"
            class="break-all font-medium text-sm">
            <span><%= item[:title] %></span>
          </p>
        </div>
        <div class="w-4/5">
          <% if item[:boolean] %>
            <% if item[:value] %>
              <img
                class="h-5 w-5"
                draggable="false"
                alt="svgImg"
                src="data:image/svg+xml;base64,"/>
            <% else %>
              <img
                class="h-5 w-5"
                draggable="false"
                alt="svgImg" src="data:image/svg+xml;base64,"/>
            <% end %>
          <% else %>
            <span class="text-sm truncate text-gray-800"><%= item[:value] %></span>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>