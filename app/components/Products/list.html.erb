<div class="w-full grid grid-cols-12">
  <% @products.each do |product| %>
    <div class="col-span-12 flex items-center" id="product_<%= product.id.to_s %>">
      <%= render Dashboard::Products::Item.new(product: product, place: @place, with_delete: true) %>
    </div>
    <%#= render Products::Item.new(product: product, in_dashboard: @in_dashboard, place: @place) %>
  <% end %>
</div>
