var element = document.querySelector("#aggregate_<%=@aggregate_id %>");
document.querySelector("#notifications").innerHTML = "";
<% if @error %>
window.addToastify("danger", "Ha ocurrido un error al eliminar la subvariante");
<% else %>
if (element) {
    console.log("Se encontr√≥ elemento");
    element.parentNode.removeChild(element);
}
window.addToastify("primary", "Se ha eliminado la subvariante");
<% end %>

window.deleteFlashs();
