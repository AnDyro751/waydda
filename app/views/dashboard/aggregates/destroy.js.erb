var element = document.querySelector("#aggregate_<%=@aggregate_id %>");
document.querySelector("#notifications").innerHTML = "";
<% if @error %>
document.querySelector("#notifications").innerHTML = "<%= j render Notifications::Alert.new(alert: "Ha ocurrido un error al eliminar la subvariante",kind: "error") %>";
<% else %>
if (element) {
    console.log("Se encontr√≥ elemento");
    element.parentNode.removeChild(element);
}
document.querySelector("#notifications").innerHTML = "<%= j render Notifications::Alert.new(alert: "Se ha eliminado la subvariante",kind: "success") %>";
<% end %>

window.deleteFlashs();
