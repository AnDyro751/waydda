<%= simple_form_for(@product, url: dashboard_product_path(@product),method: :patch) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :item_id, collection: @place.items.map { |r| [r.name,r.id] },prompt: "Selecciona el item" %>
  <%= f.button :submit %>
<% end %>