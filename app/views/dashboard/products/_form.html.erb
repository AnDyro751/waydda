<%= form_for(@product, url: dashboard_product_path(@product), method: :patch, enctype: "multipart/form-data", id: "product_edit_form") do |f| %>
  <% @presign_data[:fields].each do |name, value| %>
    <%= f.hidden_field name, value: value %>
  <% end %>
  <%= f.text_field :name %>
  <%= f.text_field :description %>
  <%= f.text_field :price %>
  <%= f.text_field :item_id, collection: @place.items.map { |r| [r.name, r.id] }, prompt: "Selecciona el item" %>
  <%= f.button :submit %>
<% end %>

