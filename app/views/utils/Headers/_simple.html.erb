<div class="w-full sticky top-0 border-b">
  <div class="flex w-full gap-4 px-6 bg-white items-center h-20 justify-between">
    <div class="w-7/12 flex items-center gap-12">
      <div class="w-auto">
        <a href="/">
          <img
            draggable="false"
            src="<%= get_image_url(20, 100, "utils/w-logo-5.png") %>"
            class="h-10"
            />
        </a>
      </div>
      <div class="w-10/12">
        <form
          class="w-full relative">
          <div class="absolute top-0 flex justify-center items-center left-0 bottom-0 h-full ml-4">
            <img
              class="h-5 opacity-75"
              alt="svgImg"
              src="data:image/svg+xml;base64,"/>
            <!--            <FaSearch className={"inline"}/>-->
          </div>
          <input type="search"
                 value="<%= params["seach"].present? ? params["seach"] : "" %>"
                 autoFocus="<%= params["seach"].present? %>"
                 class="py-3 pl-12 pr-5 rounded-full bg-main-gray focus:outline-none w-full"
                 placeholder="Busca en tu ciudad"
            />
        </form>
      </div>
    </div>
    <div
      class="w-3/12">
      <%= react_component("Shipment", props: {current_user: current_user, current_user_id: current_user ? current_user.id.to_s : nil, current_address: @current_address, locations: default_locations}, prerender: false) %>
    </div>
    <div class="w-2/12 px-0 flex">
      <%= render Header::Public::Cart.new(current_cart: @current_cart) %>
      <%#= react_component("Simple", props: {current_user: current_user, current_user_id: current_user ? current_user.id.to_s : nil, current_address: @current_address, locations: default_locations}, prerender: false) %>
    </div>
  </div>
</div>

