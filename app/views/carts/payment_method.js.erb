<% if @new_payment_method === "card" %>
document.querySelector("#cart-current-payment").innerHTML = "<%= j render Carts::PaymentMethods::Cash.new(place: @place) %>"
<% else %>
document.querySelector("#cart-current-payment").innerHTML = "<%= j render Carts::PaymentMethods::Card.new(place: @place) %>"
window.stripeCards()
<% end %>
